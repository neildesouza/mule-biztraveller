{  
 	"type": "object",
    "$schema": "http://json-schema.org/draft-03/schema",
    "id": "http://jsonschema.net",
    "required": true,
    "properties": {
      "name": {
          "type": "string",
          "required": true
      },
      "company": {
          "type": "string",
          "required": true
      },
      "title": {
          "type": "string",
          "required": true
      },
      "email": {
          "type": "string",
          "format": "email",
          "required": true
      },
      "phone": {
          "type": "string",
          "required": false
      },
      "city": {
          "type": "string",
          "required": false
      },
      "country": {
        "type": "string",
        "required": false
      },
      "postalCode": {
        "type": "string",
        "required": false
      }
    }
}
